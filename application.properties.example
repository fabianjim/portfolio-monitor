# The Tiingo API token should be set as an environment variable or in run configurations.
# https://api.tiingo.com/

tiingo-api-token=${TIINGO_API_TOKEN}


# Spring Setup

spring.application.name=portfolio-monitor

# Database 
spring.datasource.url=jdbc:postgresql://localhost:5432/financial_portfolio_db
spring.datasource.username=postgres
spring.datasource.password=your_password
spring.datasource.driver-class-name=org.postgresql.Driver
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
